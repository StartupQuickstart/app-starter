{
  "name": "startupquickstart-starter",
  "version": "0.0.1",
  "description": "Starter app built with @startupquickstart server and react client",
  "main": "index.js",
  "scripts": {
    "start": "startupquickstart start",
    "build": "startupquickstart build",
    "dev": "startupquickstart dev",
    "dev:client": "startupquickstart dev:client",
    "dev:db": "bash ./bin/dev-db.sh",
    "db:migration": "startupquickstart db:migration $1",
    "db:migrate": "startupquickstart db:migrate",
    "db:seed": "startupquickstart db:seed",
    "deps:install": "bash ./bin/install.sh",
    "deps:update": "bash ./bin/install.sh",
    "setup": "bash ./bin/setup.sh",
    "setup:gmail": "startupquickstart setup:gmail",
    "setup:secret": "startupquickstart setup:secret --env $ENV",
    "creds": "$(yawsso login --profile $AWS_PROFILE -e)"
  },
  "author": "Thomas Boles <thomas@startupquickstart.com> (https://github.com/startupquickstart)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "eslint": "^8.18.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-google": "^0.14.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/runtime": "^7.16.7",
    "@startupquickstart/server": "^0.4.61",
    "babel-plugin-root-import": "^6.6.0",
    "prompt": "^1.3.0",
    "shelljs": "^0.8.5"
  }
}
